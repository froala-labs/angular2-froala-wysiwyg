{
  "name": "angular2-froala-wysiwyg",
  "version": "2.4.1",
  "description": "Angular2 bindings for Froala WYSIWYG HTML rich text editor",
  "main": "lib/froala.directives.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c configs/bs-config.js",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/froala/angular2-froala-wysiwyg.git"
  },
  "keywords": [
    "angular2",
    "froala",
    "html",
    "text",
    "editor",
    "wysisyg",
    "jquery-plugin",
    "rich editor",
    "rich text editor",
    "rte",
    "javascript",
    "jquery"
  ],
  "author": "Florin Marius Popescu <florin@froala.com> (https://www.froala.com/)",
  "license": "https://froala.com/wysiwyg-editor/terms/",
  "bugs": {
    "url": "https://github.com/froala/angular2-froala-wysiwyg/issues"
  },
  "dependencies": {
    "froala-editor": "2.4.1",
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0",
    "angular2-in-memory-web-api": "0.0.18",
    "babel-core": "^6.11.4",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "systemjs-builder": "^0.15.25",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
